{
  "name": "bot-name",
  "repository": "https://github.com/ankhgerel/djs-template",
  "author": "Your name",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "start": "yarn build && node .",
    "start:node": "node .",
    "dev": "yarn generate && ts-node-dev -r tsconfig-paths/register --transpile-only --respawn --trace-warnings --trace-uncaught --exit-child -- ./src/index.ts",
    "generate": "prisma generate",
    "build": "tsc && tsc-alias",
    "update": "yarn upgrade-interactive",
    "lint": "eslint . --ext .ts",
    "lint:fix": "prettier --write '**/*.ts' && eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@prisma/client": "latest",
    "chalk": "4",
    "discord.js": "^14.2.0",
    "dokdo": "^0.6.1",
    "dotenv": "^16.0.1",
    "node-fetch": "2",
    "uuid": "^8.3.2",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@types/node": "^18.7.8",
    "@types/node-fetch": "^2.6.2",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.7.1",
    "prisma": "latest",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "latest"
  },
  "eslintIgnore": [
    "node_modules",
    "src/commands/",
    "example.*",
    "eslintrc.*"
  ],
  "engines": {
    "node": ">=16.9"
  },
  "packageManager": "yarn@3.2.2",
  "private": true
}
